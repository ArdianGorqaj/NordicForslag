@model IEnumerable<Forslag>
    @{
    ViewData["Title"] = "Index";
    }

                      <div class="container p-3">
                          <div class="row pt pt-4">
                              <div class=" col-6">
                                  <h2 class="text-primary">Forslags liste</h2>
                              </div>

                              <div class=" col-6 text-end">

                                  <a asp-controller="Forslag" asp-action="Opprett" class="btn btn-primary">
                                      <i class="bi bi-plus-circle"></i> &nbsp; Nytt forslag
                                  </a>
                              </div>
                          </div>
                          <br /> <br />

                          <table class="table table-bordered table-striped" style="width:100%">
                              <thead>
                                  <tr>
                                      <th>
                                          Navn
                                      </th>
                                      <th>
                                          Tittel
                                      </th>
                <th>
                    Forbedringsforslag
                </th>
                <th>
                    Årsak
                </th>
                <th>
                    Løsning
                </th>
                <th>
                    Mål
                </th>
                <th>
                    DatoRegistrert
                </th>
               
                                  </tr>

                              </thead>
                              <tbody>
                                  @foreach(var obj in Model)
            {
                                  <tr>
                                      <td width="10%">
                                          @obj.Navn
                                      </td>
                                      <td width="10%">
                                          @obj.Tittel
                                      </td>
                    <td width="20%">
                        @obj.Forbedringsforslag
                    </td>
                    <td width="40%">
                        @obj.Årsak
                    </td>
                    <td width="40%">
                        @obj.Løsning
                    </td>
                    <td width="30%">
                        @obj.Mål
                    </td>
                    <td width="20%">
                        @obj.DatoRegistrert
                    </td>
                   
                                  </tr>
            }

                              </tbody>





                          </table>
                      </div>